<templateSet group="Pandocker">
    <template name=".pdiv" value="::: {$CLASS$}&#10;$END$&#10;:::&#10;" description="Pandoc's div statement ::: {$CLASS$} :::" toReformat="false" toShortenFQNames="true">
        <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="MARKDOWN" value="true" />
        </context>
    </template>
    <template name=".rmnote" value="::: rmnote&#10;$END&#10;:::&#10;" description="Removable note ::: rmnote :::" toReformat="false" toShortenFQNames="true">
        <context>
            <option name="MARKDOWN" value="true" />
        </context>
    </template>
